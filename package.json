    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "systeminformation": "^5.12.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3"
  }
}
