{
  "name": "@matrixai/polykey",
  "version": "1.0.0",
  "homepage": "https://polykey.io",
  "author": "Matrix AI",
  "contributors": [
    {
      "name": "Roger Qiu"
    },
    {
      "name": "Aashwin Varshney"
    },
