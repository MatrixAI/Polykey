{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "identity"
    },
    "node": {
      "type": "string",
      "minLength": 1
    },
    "identity": {
      "type": "string",
      "minLength": 1
    },
    "provider": {
      "type": "string",
      "minLength": 1
    },
    "timestamp": {
      "type": "number"
    },
    "signature": {
      "type": "string",
      "minLength": 1
    }
  },
  "required": [
    "type",
    "node",
    "identity",
    "provider",
    "timestamp",
    "signature"
  ],
  "additionalProperties": false
}
